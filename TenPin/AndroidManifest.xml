<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tenpin"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo" >
        <activity
            android:name="com.example.tenpin.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.tenpin.PlayerActivity"
            android:label="@string/title_activity_player" >
        </activity>
        <activity
            android:name="com.example.tenpin.GameActivity"
            android:label="@string/title_activity_game" >
        </activity>
        <activity
            android:name="com.example.tenpin.SeriesActivity"
            android:label="@string/title_activity_series" >
        </activity>
        <activity
            android:name="com.example.tenpin.NewUserActivity"
            android:label="@string/title_activity_new_user" >
        </activity>
        <activity
            android:name="com.example.tenpin.DBManagment"
            android:label="@string/title_activity_dbmanagment" >
        </activity>
        <activity
            android:name="com.example.tenpin.BowlingKeyboard"
            android:label="@string/title_activity_bowling_keyboard" >
        </activity>
    </application>

</manifest>
